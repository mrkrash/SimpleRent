{% extends 'admin.html.twig' %}

{% block title %}Structure{% endblock %}

{% block content %}
    <h1>Structure</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ structure.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ structure.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ structure.address }}</td>
            </tr>
            <tr>
                <th>Web</th>
                <td>{{ structure.web }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_structure_index') }}">back to list</a>

    <a href="{{ path('app_structure_edit', {'id': structure.id}) }}">edit</a>

    {{ include('structure/_delete_form.html.twig') }}
{% endblock %}
