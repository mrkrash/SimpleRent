{% extends 'admin.html.twig' %}

{% block title %}Affiliate{% endblock %}

{% block content %}
    <h1>Affiliate</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ affiliate.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ affiliate.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ affiliate.address }}</td>
            </tr>
            <tr>
                <th>Web</th>
                <td>{{ affiliate.web }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_affiliate_index') }}">back to list</a>

    <a href="{{ path('app_affiliate_edit', {'id': affiliate.id}) }}">edit</a>

    {{ include('affiliate/_delete_form.html.twig') }}
{% endblock %}
